{{ form_start(form, {attr: {class: 'filter js-filter-form'}}) }}

<div class="spinner-border js-loading" role="status" aria-hidden="true" style="display: none">
    <span class="sr-only">Loading...</span>
</div>

<h4>Choisir un user</h4>
{{ form_row(form.name) }}
<h4>Choisir groupe</h4>

<h4>max</h4>
<div class="row">
    <div class="col-md-6">
        {{ form_row(form.max) }}
    </div>
    <h4>min</h4>
    <div class="col-md-6">
        {{ form_row(form.min) }}
    </div>
    <h4>groupe</h4>
    <div class="col-md-6">
        {{ form_row(form.groupe) }}
    </div>
</div>
<button type="submit" class="btn btn-primary text-white w-100 mt-5">Filtrer</button>


{{ form_end(form) }}